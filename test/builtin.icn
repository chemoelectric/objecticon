import lang

global l

procedure FncDef(s)
   put(l, s)
end

procedure main()
   local f,s
   l := []
   $include "../base/h/fdefs.h"

   every f := !l do {
      yes_no{image(f), f === proc(Proc.get_name(f),0)}
   }

   every s := Prog.get_function_names() do
      yes_no{s, s === Proc.get_name(proc(s,0))}

end
