package ipl.lstfncs

import
   ipl.genrfncs(fibseq, multiseq, primeseq),
   ipl.numbers(residue)

procedure fiblist(l, m)
   local result

   /l := 128
   /m := 8

   result := []

   every put(result, residue(fibseq(), m, 1)) \ l

   return result

end

procedure multilist(l, m)
   local result

   /l := 128
   /m := 8

   result := []

   every put(result, residue(multiseq(), m, 1)) \ l

   return result

end

procedure primelist(l, m)
   local result

   /l := 128
   /m := 8

   result := []

   every put(result, residue(primeseq(), m, 1)) \ l

   return result

end

procedure List(e, l, m)		
   local result

   l := @\l | 128	# length
   m := @\m | 8		# modulus

   result := []

   every put(result, residue(|@e, m, 1)) \ l

   return result

end
