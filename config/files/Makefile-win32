include Makedefs

.PHONY:	libref test bmark tar install install-libref

all :
	make -C base all
	make -C lib all
	make -C apps all
	make -C examples all

clean :
	make -C base clean
	make -C lib clean
	make -C apps clean
	make -C examples clean
	-$(RM) bin/*

test :
	cd test && ./runtest.sh

bmark :
	cd bmark && ./runbmark.sh

libref :
	rm -rf libref
	mkdir libref
	oidoc -d -o libref -a
