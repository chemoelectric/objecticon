import gui

class CanvasHLineUI(CanvasComponentUI)
   public override create_canvas_component(canvas)
      local t
      t := CanvasLine()
      t.set_size(scale(100))
      return t
   end

   public override get_category()
      return "Separators"
   end

   public override get_name()
      return "HLine"
   end
end

