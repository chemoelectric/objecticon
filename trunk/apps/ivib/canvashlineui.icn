class CanvasHLineUI(CanvasComponentUI)
   public create_canvas_component(canvas)
      local t
      t := CanvasLine()
      t.set_size(100)
      return t
   end

   public get_category()
      return "Separators"
   end

   public get_name()
      return "HLine"
   end
end

