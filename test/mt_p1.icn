invocable all

import lang(Class), any.pack, io

global c, what

procedure main()
   local i,s,p

   what := 50
   c := load("mt_c1") | stop("Couldn't load mt_c1")
   @c
   i := 1
   repeat {
      s := "test"||i
      p := proc(s) | break
      write("Test ", s)
      p()
      if what ~=== 50 then
         stop("what messed up")
      i +:= 1
   }
   write("exit")
end

