class CanvasEditListUI(CanvasComponentUI)
   public create_canvas_component(canvas)
      local t
      t := CanvasEditList()
      t.set_selection_list([])
      t.set_size(100)
      return t
   end

   public get_category()
      return "List"
   end

   public get_name()
      return "EditList"
   end

   public get_icon()
      return (
              $include "icon/icn19.icon"
              )
   end
end

