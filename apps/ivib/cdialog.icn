import lang
import gui

#
# Extends Dialog to enable saving of some fields.
#
class CDialog(Dialog,SelectiveObjectCodec)
   private pre_decode()
      new()
   end

   public static get_template()
      suspend ["WAttribs", "wattribs"]
   end

   public new()
      cast(self,Dialog).new()
      self.clear_wattribs()
      self.is_open_flag := 1
      return
   end
end

