class CanvasIconButtonUI(CanvasComponentUI)
   public create_canvas_component(canvas)
      local t
      t := CanvasIconButton()
      t.set_paint(CanvasImagePaint().set_cache("ivib.icon"))
      return t
   end

   public get_category()
      return "Buttons"
   end

   public get_name()
      return "IconButton"
   end

   public get_icon()
      return "ivib.icn5"
   end
end

