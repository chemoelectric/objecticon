package gui

abstract class DynamicMenu(Menu)
   public show(x, y, xa, ya)
      /self.temp_win | fail

      # Clear current menu contents.
      every (!self.children).finally()

      self.children := []
      populate()

      # Init new ones
      every (!self.children).initially()

      cast(self,Menu).show(x, y, xa, ya)
   end

   public abstract populate()
end

