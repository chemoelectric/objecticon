class CanvasTextDisplayUI(CanvasComponentUI)
   public create_canvas_component(canvas)
      local t
      t := CanvasTextDisplay()
      t.set_contents([])
      t.set_size(200, 150)
      return t
   end

   public get_category()
      return "Text"
   end

   public get_name()
      return "TextDisplay"
   end
end

