import gui

class CanvasTreeUI(CanvasComponentUI)
   public override create_canvas_component(canvas)
      local t
      t := CanvasTree()
      t.set_size(scale(180), scale(120))
      return t
   end

   public override get_category()
      return "Other"
   end

   public override get_name()
      return "Tree"
   end

   public override get_icon()
      return "ivib.icn36"
   end
end

