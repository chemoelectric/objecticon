class CanvasIconUI(CanvasComponentUI)
   public create_canvas_component(canvas)
      local t
      t := CanvasIcon()
      t.set_paint(CanvasImagePaint().set_cache("ivib.icon"))
      return t
   end

   public get_category()
      return "Image"
   end

   public get_name()
      return "Icon"
   end

   public get_icon()
      return "ivib.icn6"
   end
end

