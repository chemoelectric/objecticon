class CanvasHSliderUI(CanvasComponentUI)
   public create_canvas_component(canvas)
      local t
      t := CanvasSlider()
      t.set_constraint("x_fill", &yes)
      t.set_constraint("x_weight", "1")
      t.set_range(0, 100)
      t.set_value(0)
      t.set_size(100)
      t.set_is_horizontal(&yes)
      return t
   end

   public get_category()
      return "Slider"
   end

   public get_name()
      return "HSlider"
   end
end

