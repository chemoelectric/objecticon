         set_constraint("anchor", "w").
         set_constraint("eol", &yes).
         set_constraint("fill", "h").
         set_constraint("x_fill", &yes).
         set_paint(paint_2)
      self.add(label_5)
      paint_3 := TextPaint("Dialog size:")
