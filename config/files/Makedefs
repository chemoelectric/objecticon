.PHONY:         all clean
.PRECIOUS:      %.u

OIHOME=@CONFIG_DIR@
OIBIN=$(OIHOME)/bin
OILIB=$(OIHOME)/lib

CDBG=-ggdb
LDFLAGS=@LDFLAGS@
LIBS=@LIBS@
CFLAGS=$(CWARN) $(CDBG) @CFLAGS@
CPPFLAGS=@CPPFLAGS@

RM=rm -f
RMDIR=rm -rf
MKDIR=mkdir
CP=cp
INSTALL=@INSTALL@
LN_S=@LN_S@
@SET_MAKE@

export OIPATH:=$(OILIB)/main:$(OILIB)/gui:$(OILIB)/xml:$(OILIB)/parser:$(OILIB)/ipl
export OLPATH:=$(OILIB)/incl
export PATH:=$(OIBIN):$(PATH)

%.u: %.icn
	oit -c -s $<
