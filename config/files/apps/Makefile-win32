include ../Makedefs

PROGS=  ieval fiximports memdebug memorytracker
PROGSRC=$(PROGS:=.icn)
PROGEXE=$(PROGS:=.exe)

all :	$(PROGS)
	$(MAKE) -C ivib all
	$(MAKE) -C oidoc all

clean :
	$(RM) *.u packages.txt $(PROGEXE)
	$(MAKE) -C ivib clean
	$(MAKE) -C oidoc clean

%: 	%.icn
	oit -s $<
	cp $@ "$(OI_BIN)"
