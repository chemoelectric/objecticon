include ../../../Makedefs

export OI_PATH:=$(OI_PATH);$(OI_HOME)\apps\ivib

PROGSRC:=$(wildcard *.icn)
PROGU=$(PROGSRC:.icn=.u)
PROGS=$(PROGSRC:.icn=)
PROGEXE=$(PROGS:=.exe)

all:    $(PROGS)

clean:
	rm -f *.u packages.txt $(PROGEXE)

$(PROGU): %.u: %.icn
	oit -snc $(PROGSRC)

%:      %.u
	oit -b2 -o $@ $@.u
