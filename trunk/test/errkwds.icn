#SRC: JCON
#OPT: -fe

import io

procedure main()
	write(&error)
	write(&errornumber | "fail")
	write(&errortext | "fail")
	write(image(&errorvalue) | "fail")
	&error := 1
	write(&error)
	write(&null() | "fail")
	write(&error)
	write(&errornumber | "fail")
	write(&errortext | "fail")
	write(image(&errorvalue) | "fail")
	write(&error)
	write(&errornumber | "fail")
	write(&errortext | "fail")
	write(image(&errorvalue) | "fail")
	errorclear()
	write(&error)
	write(&errornumber | "fail")
	write(&errortext | "fail")
	write(image(&errorvalue) | "fail")
end
