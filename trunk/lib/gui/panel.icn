package gui


#
# A Panel of components.
#
class Panel(Component)
   public new()
      cast(self,Component).new()
      self.set_constraint("x_fill", &yes)
      self.set_constraint("y_fill", &yes)
      self.set_constraint("x_weight", 1.0)
      self.set_constraint("y_weight",  1.0)
      return
   end
end

