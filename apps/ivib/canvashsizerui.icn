import gui

class CanvasHSizerUI(CanvasComponentUI)
   public create_canvas_component(canvas)
      local t
      t := CanvasSizer()
      t.set_size(scale(100))
      t.set_is_horizontal(&yes)
      return t
   end

   public get_category()
      return "Separators"
   end

   public get_name()
      return "HSizer"
   end
end

