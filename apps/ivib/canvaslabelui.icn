class CanvasLabelUI(CanvasComponentUI)
   public override create_canvas_component(canvas)
      local t
      t := CanvasLabel()
      t.set_paint(CanvasTextPaint("Label"))
      return t
   end

   public override get_category()
      return "Text"
   end

   public override get_name()
      return "Label"
   end

   public override get_icon()
      return "ivib.icn2"
   end
end

