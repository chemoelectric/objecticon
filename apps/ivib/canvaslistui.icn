import gui

class CanvasListUI(CanvasComponentUI)
   public create_canvas_component(canvas)
      local t
      t := CanvasList()
      t.set_selection_list([])
      t.set_size(scale(100))
      return t
   end

   public get_category()
      return "List"
   end

   public get_name()
      return "List"
   end

   public get_icon()
      return "ivib.icn18"
   end
end

