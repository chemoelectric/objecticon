include ../Makedefs

PROGS=  ieval fiximports memdebug memorytracker
SRC=$(PROGS:=.icn)
BIN=$(addprefix ../bin/, $(PROGS))

all :	$(BIN)
	$(MAKE) -C ivib all
	$(MAKE) -C oidoc all

clean :
	$(RM) *.u packages.txt $(BIN)
	$(MAKE) -C ivib clean
	$(MAKE) -C oidoc clean

../bin/%: 	%.icn
	oit -s -o $@ $<
