class CanvasTreeUI(CanvasComponentUI)
   public create_canvas_component(canvas)
      local t
      t := CanvasTree()
      t.set_size(180, 120)
      return t
   end

   public get_category()
      return "Other"
   end

   public get_name()
      return "Tree"
   end

   public get_icon()
      return (
      $include "icon/icn36.icon"
      )
   end
end

