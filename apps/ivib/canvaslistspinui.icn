class CanvasListSpinUI(CanvasComponentUI)
   public create_canvas_component(canvas)
      local t
      t := CanvasListSpin()
      t.set_selection_list([])
      t.set_size(100)
      return t
   end

   public get_category()
      return "Spin"
   end

   public get_name()
      return "ListSpin"
   end
end

