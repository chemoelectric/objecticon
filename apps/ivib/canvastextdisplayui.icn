import gui

class CanvasTextDisplayUI(CanvasComponentUI)
   public override create_canvas_component(canvas)
      local t
      t := CanvasTextDisplay()
      t.set_contents([])
      t.set_size(scale(200), scale(150))
      return t
   end

   public override get_category()
      return "Text"
   end

   public override get_name()
      return "TextDisplay"
   end
end

