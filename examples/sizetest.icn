import
   gui(Align, CheckBox, Dialog, Dispatcher,
       Event, GridLayout, Label, Panel,
       TextButton, TextField, TextPaint)

class Untitled(Dialog)
   private
      label_5,
      text_field_3,
      text_button_4,
      panel_1,
      check_box_1,
      ticker,
      label_1, 
      label_2, 
      layout_1, 
      text_button_1, 
      text_button_2, text_button_3, text_field_1, text_field_2, label_3, label_4,
      text_field_4,
      text_button_5,
      label_7,
      label_6,
      label_8

   public component_setup()
      self.setup()
   end

   public end_dialog()
   end

   public init_dialog()
      ticker := Dispatcher.new_task{tick()}
      ticker.start()
   end

   public tick()
      local t
      repeat {
         t := win.get_geometry()
         label_1.set_label("Geom:" || t[1] || "," || t[2] || " " || t[3] || "x" || t[4])
         t := win.get_min_size()
         label_4.set_label("Min size:" || t[1] || "," || t[2])
         t := win.get_max_size()
         label_5.set_label("Max size:" || t[1] || "," || t[2])
         label_3.set_label("Dialog w,h:" || w || "," || h)
         t := win.query_root_pointer()
         label_6.set_label("Root mouse:"||t[1]||","||t[2])
         t := win.query_pointer()
         label_7.set_label("Win mouse:"||t[1]||","||t[2])
         t := win.get_display_size()
         label_8.set_label("Display size:"||t[1]||","||t[2])
         ticker.sleep(250)
      }
   end

   private parse_pair(s)
      local a, b
      if s ? {
         a := integer(tab(many(&digits ++ '-'))) &
            ="," &
            b := integer(tab(many(&digits ++ '-')))
      } then
         return [a, b]
   end

   public on_text_button_1(ev)
      win.set_min_size!(parse_pair(text_field_1.get_contents()))
   end

   private on_text_button_4(ev)
      win.set_max_size!(parse_pair(text_field_3.get_contents()))
   end

   public on_text_button_2(ev)
      resize_win!(parse_pair(text_field_2.get_contents()))
   end

   public on_text_button_3(ev)
      dispose()
   end

   private on_check_box_1(ev)
      win.set_resize(check_box_1.get_status())
   end

   private on_text_button_5(ev)
      win.set_pos!(parse_pair(text_field_4.get_contents()))
   end

   private setup()
      local paint_2, paint_7, paint_11, paint_4, paint_1, paint_9, paint_6, paint_10, paint_3, paint, paint_8, paint_12, paint_5, layout_2
      self.set_pos(100, 100)
      self.set_resize(&yes)
      paint := TextPaint("Geom:")
      label_1 := Label().
         set_size("275").
         clear_constraints().
         set_constraint("anchor", "w").
         set_constraint("eol", &yes).
         set_constraint("fill", "h").
         set_constraint("x_fill", &yes).
         set_constraint("x_weight", 1.0).
         set_paint(paint)
      self.add(label_1)
      paint_1 := TextPaint("Min size:")
      label_4 := Label().
         clear_constraints().
         set_constraint("anchor", "w").
         set_constraint("eol", &yes).
         set_constraint("fill", "h").
         set_constraint("x_fill", &yes).
         set_paint(paint_1)
      self.add(label_4)
      paint_2 := TextPaint("Max size:")
      label_5 := Label().
