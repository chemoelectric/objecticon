#SRC: JCON

import io

#  prime number generation using co-expressions

procedure main()
   local s, x

   s := create (2 to 100)
   while (x := @s) do {
      write(x)
      s := create sieve(x, s)
   }
end

procedure sieve(x, s)
   local t

   while t := @s do {
      if t % x ~= 0 then suspend t
   }
end
