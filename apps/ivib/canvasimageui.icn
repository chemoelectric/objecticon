import gui

class CanvasImageUI(CanvasComponentUI)
   public create_canvas_component(canvas)
      local t
      t := CanvasImage()
      t.set_size(scale(60), scale(60))
      return t
   end

   public get_category()
      return "Image"
   end

   public get_name()
      return "Image"
   end

   public get_icon()
      return "ivib.icn7"
   end
end

