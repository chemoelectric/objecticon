$start ensure_class_initialized
      Op_Custom c:check_if_uninitialized
         Op_FrameVar arg:0
         Exit
      Op_Custom c:set_class_state
         Op_FrameVar arg:0
         Op_Int Initializing
      Op_Move
         Op_Tmp tmp:0
         Op_Int 0
Loop: 
      Op_Custom c:for_class_supers
         Op_FrameVar arg:0
         Op_Tmp tmp:0
         Op_Tmp tmp:1
         Brk
      Op_Custom c:ensure_class_initialized
         Op_Tmp tmp:1
      Op_Goto 
         Loop
Brk:  
      Op_Mark
         mark:0
      Op_Custom c:invoke_class_init
         Op_FrameVar arg:0
         InitFail
      Op_Unmark
         mark:0
InitFail:
      Op_Custom c:set_class_state
         Op_FrameVar arg:0
         Op_Int Initialized
Exit: 
      Op_Fail
$end

$start construct_object
    Op_Custom c:ensure_class_initialized
       Op_FrameVar arg:0
    Op_Resume
       clo:0
       NewFail
    Op_Unmark
       mark:0
    Op_Custom c:set_object_state
       Op_FrameVar arg:1
       Op_Int Initialized
    Op_Return
       Op_FrameVar arg:1
NewFail:
    # In case a reference to the object escaped from the constructor, set
    # its state to Initialized.
    Op_Custom c:set_object_state
       Op_FrameVar arg:1
       Op_Int Initialized
    Op_Fail
$end

#
# Construct object closure for class with no "new" function
#
$start construct_object0
    Op_Custom c:ensure_class_initialized
       Op_FrameVar arg:0
    Op_Custom c:set_object_state
       Op_FrameVar arg:1
       Op_Int Initialized
    Op_Return
       Op_FrameVar arg:1
    Op_Fail
$end

$start generate_arg
    Op_Return
       Op_FrameVar arg:0
    Op_Fail
$end

#
# This frame simply initializes the given class, then pops itself and repeats
# the last instruction in the calling frame.
#
$start initialize_class_and_repeat
    Op_Custom c:ensure_class_initialized
       Op_FrameVar arg:0
    Op_PopRepeat
$end

$start get_impl
    Op_Custom c:simple_access
         Op_Tmp tmp:0
         Op_FrameVar arg:0
         Op_FrameVar arg:1
         Op_Int 0
         FailAccess
    Op_Custom c:set_c_frame_return
         Op_Tmp tmp:0

FailAccess:
    # Will only get here if &error != 0.  Call handle_access_failure
    # with quiet = 1.
    Op_Custom c:handle_access_failure
         Op_Int 1
$end

$start getf_impl
    Op_Custom c:simple_access
         Op_Tmp tmp:0
         Op_FrameVar arg:0
         Op_FrameVar arg:1
         Op_Int 1
         FailAccess
    Op_Custom c:set_c_frame_return
         Op_Tmp tmp:0

FailAccess:
    Op_Custom c:handle_access_failure
         Op_FrameVar arg:2
$end

