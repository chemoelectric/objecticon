#  This program lists all the different words in the input text.
#  The definition of a "word" is naive.

procedure main()
   local words,text
   words := set()
   while text := read() do
      text ? while tab(upto(&letters)) do
         insert(words,tab(many(&letters)))
   every write(!sort(words))
end
