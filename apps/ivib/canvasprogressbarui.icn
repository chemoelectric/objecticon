import gui

class CanvasProgressBarUI(CanvasComponentUI)
   public create_canvas_component(canvas)
      local t
      t := CanvasProgressBar()
      t.set_size(scale(100))
      return t
   end

   public get_category()
      return "Other"
   end

   public get_name()
      return "ProgressBar"
   end
end

