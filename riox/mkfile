< ../mkdefs

OFILES=\
	rio.$O\
	data.$O\
	fsys.$O\
	scrl.$O\
	time.$O\
	util.$O\
	wctl.$O\
	wind.$O\
	xfid.$O\
	frbox.$O\
	frdelete.$O\
	frdraw.$O\
	frinit.$O\
	frinsert.$O\
	frptofchar.$O\
	frselect.$O\
	frutil.$O\

all:V:  $O.out

$O.out:  $OFILES
	$LD -o riox $OFILES
	rm -f $OI_BIN/riox
	cp riox $OI_BIN

clean:V:	
	rm -f $OFILES riox
